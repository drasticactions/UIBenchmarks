<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UIBenchmarks.MauiUI.ImagePlaceholderPage"
             xmlns:local="clr-namespace:UIBenchmarks.MauiUI"
             Title="ImagePlaceholderPage">
    <ListView ItemsSource="{Binding ImageUrls}">
        <ListView.ItemTemplate>
            <DataTemplate>
                <ViewCell Height="300">
                    <local:ImageContentView Uri="{Binding}" WidthRequest="200" HeightRequest="300"/>
                </ViewCell>
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>